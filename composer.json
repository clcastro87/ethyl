{
    "name": "clcastro87/ethyl",
    "version": "1.0.2",
    "type": "library",
    "description": "Easy ETL Pipelines done right.",
    "require": {
        "ext-json": "*",
        "ext-pdo": "*",
        "ext-simplexml": "*",
        "ext-libxml": "*",
        "league/csv": "^9.6",
        "league/flysystem": "^1.0",
        "league/pipeline": "^1.0",
        "prewk/xml-string-streamer": "^0.13.0"
    },
    "require-dev": {
        "phpunit/phpunit": "9.*"
    },
    "config": {
        "sort-packages": true
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Carlos Luis Castro Marquez",
            "email": "carlosluiscastro@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Ethyl\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ethyl\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": [
            "vendor/bin/phpunit"
        ]
    },
    "archive": {
        "exclude": ["/examples"]
    }
}
